// Вар 3 - Средний

    Console.WriteLine("        x        y        ");
    for (double x = -5; x <= 10; x += 0.2) 
    {
        double y = 0;
        if (x < 12.5) y = x * x;
        else if (x >= 0 && x <= 1) y = Math.Exp(x) + 5 + Math.Cos(0.001 * x);
        else y = x * x - 3 * x + 2.5 * x * x;
        Console.WriteLine($"        {x:F1}        {y:F2}");
    }
